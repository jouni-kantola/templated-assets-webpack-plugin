{
  "name": "templated-assets-webpack-plugin",
  "version": "1.3.1",
  "description": "webpack plugin for creating assets to be used with server rendered web frameworks.",
  "main": "index.js",
  "dependencies": {
    "is": "^3.2.1",
    "mkdirp": "^0.5.1",
    "webpack-sources": "^1.0.2"
  },
  "devDependencies": {
    "ava": "^0.23.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "css-loader": "^2.1.1",
    "escope": "^3.6.0",
    "eslint": "^4.11.0",
    "eslint-plugin-prettier": "^2.3.1",
    "mini-css-extract-plugin": "^0.8.0",
    "husky": "^0.14.3",
    "is-thirteen": "^2.0.0",
    "lint-staged": "^4.3.0",
    "mock-fs": "^4.13.0",
    "nyc": "^11.3.0",
    "prettier": "^1.8.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jouni-kantola/templated-assets-webpack-plugin.git"
  },
  "keywords": [
    "webpack",
    "template",
    "assets",
    "inline",
    "ssr"
  ],
  "author": "Jouni Kantola <jouni@kantola.se> (https://github.com/jouni-kantola)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jouni-kantola/templated-assets-webpack-plugin/issues"
  },
  "homepage": "https://github.com/jouni-kantola/templated-assets-webpack-plugin#readme",
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "ava test/*-test.js",
    "coverage": "nyc --check-coverage npm test",
    "qa": "yarn lint & yarn coverage",
    "example": "webpack --config ./example/webpack.config.js",
    "precommit": "lint-staged"
  },
  "prettier": {
    "tabWidth": 2,
    "trailingComma": "none",
    "singleQuote": false
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "nyc": {
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        80,
        95
      ],
      "statements": [
        80,
        95
      ]
    }
  }
}
