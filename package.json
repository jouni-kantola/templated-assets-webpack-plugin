{
  "name": "templated-assets-webpack-plugin",
  "version": "1.1.0",
  "description": "webpack plugin for generating templated assets, suitable for i.e. server-side rendering.",
  "main": "index.js",
  "dependencies": {
    "webpack-sources": "^1.0.2"
  },
  "devDependencies": {
    "ava": "^0.19.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "chunk-manifest-webpack-plugin": "^1.0.0",
    "css-loader": "^0.28.7",
    "eslint": "^3.19.0",
    "eslint-plugin-prettier": "^2.0.1",
    "extract-text-webpack-plugin": "2.1.2",
    "husky": "^0.13.3",
    "is-thirteen": "^2.0.0",
    "lint-staged": "^3.4.0",
    "mkdirp": "^0.5.1",
    "mock-fs": "^4.2.0",
    "nyc": "^10.2.0",
    "prettier": "^0.22.0",
    "webpack": "^2.3.3",
    "webpack-chunk-hash": "^0.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jouni-kantola/templated-assets-webpack-plugin.git"
  },
  "keywords": [
    "webpack",
    "template",
    "assets",
    "inline",
    "ssr"
  ],
  "author": "Jouni Kantola <jouni@kantola.se> (https://github.com/jouni-kantola)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jouni-kantola/templated-assets-webpack-plugin/issues"
  },
  "homepage": "https://github.com/jouni-kantola/templated-assets-webpack-plugin#readme",
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "lint": "node_modules/.bin/eslint .",
    "test": "ava test/*-test.js",
    "coverage": "node_modules/.bin/nyc --check-coverage npm test",
    "qa": "npm run lint & npm run coverage",
    "example": "node_modules/.bin/webpack --config ./example/webpack.config.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "nyc": {
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        80,
        95
      ],
      "statements": [
        80,
        95
      ]
    }
  }
}
