{
  "name": "templated-assets-webpack-plugin",
  "version": "2.0.0-beta.2",
  "description": "webpack plugin for creating assets to be used with server rendered web frameworks.",
  "main": "index.js",
  "dependencies": {
    "is": "^3.3.0",
    "mkdirp": "^1.0.4",
    "webpack-sources": "^1.4.3"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "@babel/core": "^7.11.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.11.5",
    "css-loader": "^4.3.0",
    "escope": "^3.6.0",
    "eslint": "^7.9.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "is-thirteen": "^2.0.0",
    "lint-staged": "^10.4.0",
    "mini-css-extract-plugin": "^0.11.2",
    "mock-fs": "^4.13.0",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "webpack": "^5.0.0-beta.30",
    "webpack-cli": "^3.3.12"
  },
  "peerDependencies": {
    "webpack": "^3.0.0 || ^4.0.0 || ^5.0.0"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jouni-kantola/templated-assets-webpack-plugin.git"
  },
  "keywords": [
    "webpack",
    "template",
    "assets",
    "inline",
    "ssr"
  ],
  "author": "Jouni Kantola <jouni@kantola.se> (https://github.com/jouni-kantola)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jouni-kantola/templated-assets-webpack-plugin/issues"
  },
  "homepage": "https://github.com/jouni-kantola/templated-assets-webpack-plugin#readme",
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "ava test/*-test.js",
    "coverage": "nyc --check-coverage npm test",
    "qa": "yarn lint & yarn coverage",
    "example": "webpack -p --config ./example/webpack.config.js",
    "precommit": "lint-staged"
  },
  "prettier": {
    "tabWidth": 2,
    "trailingComma": "none",
    "singleQuote": false,
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "nyc": {
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        80,
        95
      ],
      "statements": [
        80,
        95
      ]
    }
  }
}
